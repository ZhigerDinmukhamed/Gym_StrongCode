# Конфигурация Gym StrongCode API

# База данных
DATABASE_PATH=gym_strongcode.db

# Адрес сервера
SERVER_ADDRESS=:8080

# JWT секретный ключ (ОБЯЗАТЕЛЬНО измените в продакшене!)
# Сгенерируйте надёжный ключ: openssl rand -base64 32
JWT_SECRET=strongcode-super-secret-key-change-in-production

# Окружение (development/production)
ENVIRONMENT=development

# Логирование
LOG_LEVEL=info
LOG_FILE=logs/app.log